#include<stdio.h>

main()
{
	int i,j;

	putchar ('Q');
	putchar ('1');
	putchar (',');
	putchar ('\n');

	for (i = 0; i <= 5; i++)
	{
		putchar('*');
	}

	putchar ('\n');
	putchar ('Q');
	putchar ('2');
	putchar (',');
	putchar ('\n');
	putchar ('\n');
	for (i = 0; i <= 5; i++)
	{
			putchar('*');
			putchar('\n');
	}
	putchar ('\n');
	putchar ('Q');
	putchar ('3');
	putchar (',');
	putchar ('\n');
	putchar ('\n');

	for (i = 0; i <= 5; i++)
	{
		for(j=0; j <=5; j++)
		{
			putchar('*');
		}
		putchar('\n');

	}

	putchar ('\n');
	putchar ('Q');
	putchar ('4');
	putchar (',');
	putchar ('\n');
	putchar ('\n');

			for(j=0; j <=5; j++)
			{
				putchar('1');
				
			}
			putchar('\n');
			for(j=0; j <=5; j++)
			{
				putchar('2');
				
			}
			putchar('\n');
			for(j=0; j <=5; j++)
			{
				putchar('3');
				
			}
			putchar('\n');
			for(j=0; j <=5; j++)
			{
				putchar('4');
				
			}
			putchar('\n');

			for(j=0; j <=5; j++)
			{
				putchar('5');
				
			}
			putchar('\n');

		putchar('\n');
}
